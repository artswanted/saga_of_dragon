<?xml version="1.0" encoding="euc-kr"?>
<EVENT_SCRIPT>
	<!-- 별자리 일반 입장-->

	<!-- 전체 UI 숨기기 -->
	<CMD TYPE="DOSTRING" COMMAND="UIOff(false)"/>

	<!-- 유닛 숨기기 -->
	<CMD TYPE="ShowActor" UnitType="UT_PLAYER" Show="false" />
	<CMD TYPE="ShowActor" UnitType="UT_SUMMONED" Show="false"/>
	<CMD TYPE="ShowActor" UnitType="UT_ENTITY" Show="false"/>
	<CMD TYPE="ShowActor" UnitType="UT_NPC" Show="false"/>
	<CMD TYPE="ShowActor" UnitType="UT_MONSTER" Show="false"/>

	
	<!-- 카메라로 연출 -->
	<CMD TYPE="SetCameraModeEvent"/>

	<CMD TYPE="EscScript" EventID="980101"/>

	<CMD TYPE="MoveCamToTrigger" Target="event_start_trigger" MoveTime="0" CamActorDistance="1000" CamMoveUpDistance="0"/>
	<CMD TYPE="WaitCamera"/>
	<CMD TYPE="MoveCamToTrigger" Target="event_start_trigger" MoveTime="3" CamActorDistance="600" CamMoveUpDistance="0"/>
	<CMD TYPE="WaitCamera"/>

	<!-- 별자리 미니맵 연출 --> 

	<CMD TYPE="CallUI" UIWndName="FRM_CONSTELLATION_MINIMAP_THUMBNAIL"/>
	<CMD TYPE="WaitShowMapThumbnail"/>

	<!-- 여기까지만 ESC키 먹힘 -->
	<CMD TYPE="ClearEscScript"/>
	<!-- 꼭 해야 되는 연출 -->
	<CMD TYPE="ActivateEvent" EventId="980101"/>


	
</EVENT_SCRIPT>