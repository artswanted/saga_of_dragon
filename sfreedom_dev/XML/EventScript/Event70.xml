<?xml version="1.0" encoding="euc-kr"?>
<!-- 파리스 소환석 관련 스크립트 -->
<EVENT_SCRIPT>
    <CMD type="CloseUI" UIWndName = "FRM_REST_MON_NUM"/>
    <CMD TYPE="LockPlayerInputMove" Lock="false"/>
    <CMD type="WaitTime" time="6"/>
     <CMD TYPE="stopBGM" SoundNo = "0"/>
     <CMD type="WaitTime" time="3"/>
     <CMD type="CallUI" UIWndName = "FRM_MISSION_END"/>
     <CMD TYPE="PlaySound" SpecialActor="ME"  SlotNumber="1" SoundID="Guild_LV_UP"/>
     <CMD type="waittime" time="5"/>
     <CMD TYPE="StopSound" SpecialActor="ME"  SlotNumber="1"/>
     <CMD type="waittime" time="1"/>

    <CMD TYPE="LockPlayerInput" Lock="TRUE"/>
    <CMD TYPE="LockPlayerInputMove" Lock="TRUE"/>
    <CMD TYPE="setmoviescene" enable="true"/>
    <CMD type="waittime" time="0"/>
    <CMD TYPE="stopBGM" SoundNo = "0"/> 
    <CMD TYPE="SetCameraModeEvent"/>

    <CMD TYPE="Fade" FadeColor = "0,0,0" FadeTime = "3" FadeIn = "false"/>
    <CMD type="WaitFade" time="2"/>
    
    <CMD TYPE="MoveCamToActor" SpecialActor ="ME" MoveTime="1"/>
    
    <CMD TYPE="WaitCamera"/>
    
    <CMD TYPE="SetCameraModeFollow" SpecialActor="ME"/>
    <CMD TYPE="setmoviescene" enable="false" Time="1"/>
    <CMD type="waittime" time="0"/>
    <CMD TYPE="LockPlayerInput" Lock="FALSE"/>
    <CMD TYPE="LockPlayerInputMove" Lock="false"/>
    <CMD TYPE="MapMove" MapID ="9010210" SpawnIndex="2" />
    <CMD TYPE="Fade" FadeColor = "0,0,0" FadeTime = "2" FadeIn = "true"/>
    <CMD type="WaitFade" time="1"/>
</EVENT_SCRIPT>
