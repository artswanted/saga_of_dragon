	<CMD TYPE="MoveCamToActor" SpecialActor="ME" MoveTIME="20" CamActorDistance="500" CamMoveUpDistance="30" HorizRotAngle="-50" VertRotAngle="25"/>
	<CMD TYPE="WAITCamera"/>
	<CMD TYPE="FADE" FADEColor="0,0,0" FADETIME="1" FADEIn="FALSE"/>
	<CMD TYPE="WAITFADE" TIME="1"/>
	<CMD TYPE="setmoviescene" enable="FALSE" TIME="1"/>
	<CMD TYPE="MoveCamToActor" SpecialActor="ME" MoveTIME="1"/>
	<CMD TYPE="WAITTIME" TIME="2"/>
	<CMD TYPE="DoScript" OBJECTID="ActivatePremiumUI()"/><!-- 프리미엄 UI 열기 -->
	<CMD TYPE="FADE" FADEColor="0,0,0" FADETIME="1" FADEIn="TRUE"/>
	<CMD TYPE="WAITFADE" TIME="1"/>
	<CMD TYPE="WAITCamera"/>
	<CMD TYPE="CallUI" UIWndName="FRM_GOGO"/> <!--GOGO UI 출력-->
	<CMD TYPE="SetCameraModeFollow" SpecialActor="ME"/>
	<CMD TYPE="WAITTIME" TIME="0"/>
    <CMD TYPE="LockPlayerInput" Lock="FALSE"/>
    <CMD TYPE="LockPlayerInputMove" Lock="FALSE"/>
</EVENT_SCRIPT>