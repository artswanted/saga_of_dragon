		<BUTTON ID="BTN_CLOSE" X="457" Y="447" Z="2" W="111" H="41" IS_VISIBLE="1">
			<ATTR TEXT="400542" T_X="55" T_Y="12" T_CENTER="1" FONT="FTtitle_14" T_OUTLINE="1" BTN_ENABLE_CHANGE_COLOR="1" BTN_T_COLOR_TYPE="9" OUTLINE_COLOR_HEX="0xFF5E2F00"/>
			<ATTR BTN_IMG="../Data/6_ui/Common/cmBtLong.tga" BTN_IMG_W="222" BTN_IMG_H="82"/>
			<SCRIPT ID="ON_L_BTN_DOWN">
				CloseUI("FRM_MISSION_QUEST")
			</SCRIPT>
		</BUTTON>
		<SCRIPT ID="ON_CALL">
			FMQ_Card_Num = 0;
			FMQ_StartTime = g_world:GetAccumTime()
		</SCRIPT>
		<SCRIPT ID="ON_DISPLAY">
			local FMQ_TickTime = g_world:GetAccumTime() - FMQ_StartTime
			if FMQ_TickTime > 5 then
				CallUI("FRM_MISSION_START")
				CloseUI("FRM_MISSION_QUEST")
			end
			local TextWnd = UISelf:GetControl("FRM_SMALL_TEXT")
			if TextWnd ~= nil then
				MQCard_TimeText(TextWnd, (5 - FMQ_TickTime))
			end
		</SCRIPT>
	</FORM>
	<FORM ID="FRM_MISSION_QUEST_TURN">
		<ATTR X="0" Y="0" W="123" H="220" IS_VISIBLE="1"/>
		<ATTR IMG="../Data/6_ui/quest/qsMcardAni.tga" IMG_W="492" IMG_H="440" U="4" V="2"/>
		<SCRIPT ID="ON_CALL">
			FMQTTime = g_world:GetAccumTime()
			UISelf:SetUVIndex(1)
		</SCRIPT>
		<SCRIPT ID="ON_DISPLAY">
			local UVIndex = UISelf:GetUVIndex()
			if UVIndex == 8 then
				UISelf:SetUVIndex(1)
			else
				UISelf:SetUVIndex(UVIndex + 1)
			end
			if g_world:GetAccumTime() - FMQTTime > 0.8 then
				if MQCard_ForwardID(UISelf, "FRM_MISSION_QUEST_SCALE") == true then
					CloseUI("FRM_MISSION_QUEST_TURN")
				end
			end
		</SCRIPT>
	</FORM>
	<FORM ID="FRM_MISSION_QUEST_SCALE">
		<ATTR X="0" Y="0" W="123" H="220"/>
		<ATTR IMG="../Data/6_ui/quest/qsMcardFront.tga" IMG_W="123" IMG_H="220"/>
		<SCRIPT ID="ON_CALL">
			g_kFMQLTime = g_world:GetAccumTime()
			FMQLScale = 1.0
		</SCRIPT>
		<SCRIPT ID="ON_DISPLAY">
			FMQLScale = FMQLScale + 0.15
			MQCard_Scale(UISelf, g_kFMQLTime, FMQLScale)
		</SCRIPT>
	</FORM>
