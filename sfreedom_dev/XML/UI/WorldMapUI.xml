<XUI>
	<FORM ID = "FRM_WORLDMAP">
		<ATTR X = "0" Y = "0" W ="1023" H ="767" USE_ALPHA_ANI = "0"/>
		<SCRIPT ID ="ON_CALL">
			if( CheckNil(g_world == nil) ) then return false end
			if( CheckNil(g_world:IsNil()) ) then return false end
			g_world:SetActivaingWorldMap(true)
			UISelf:InitWorldMap()
		</SCRIPT>
		<SCRIPT ID = "ON_DISPLAY">
			UISelf:DrawWorldMap()
			UISelf:MouseOverWorldMap(GetCursorPos())
		</SCRIPT>
		<SCRIPT ID = "ON_TICK"> 
			UISelf:PulseWorldMap()
		</SCRIPT>
		<SCRIPT ID ="ON_CLOSE">
			UISelf:TermWorldMap()
			CloseToolTip()
			if( CheckNil(g_world == nil) ) then return false end
			if( CheckNil(g_world:IsNil()) ) then return false end
			g_world:SetActivaingWorldMap(false)
			local kMapInfo = GetUIWnd("FRM_WORLDMAP_INFORMATION")
			if kMapInfo:IsNil() == false then
				kMapInfo:Close()
			end
		</SCRIPT>
		<SCRIPT ID = "ON_L_BTN_UP">
			UISelf:InSideWorldMap()
			UISelf:MouseClickWorldMap(GetCursorPos())
		</SCRIPT>
		<SCRIPT ID = "ON_R_BTN_UP">	UISelf:OutSideWorldMap() </SCRIPT>
		<HOTKEY ID = "1"> UISelf:Close() return true </HOTKEY>
		<FORM ID="FRM_TEXT" X="535" Y="45" Z="6" W="212" H="108" IGNORE_PARENT_RECT = "1">
