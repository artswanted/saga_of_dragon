						<SCRIPT ID = "ON_DISPLAY">
							local kSize = UISelf:GetSize()
							if g_bOnCooking == nil or g_bOnCooking == false then
								UISelf:SetImgSize(kSize)
								wnd = ItemCookingWnd(UISelf)
								wnd:SetNowCooking(false)
								return
							end
							if g_fCookingStartTime == nil then
								wnd:SetNowCooking(false)
								return
							end
							local fDelta = GetAccumTime() - g_fCookingStartTime
							if fDelta >= 2 then
								g_bOnCooking = false
								wnd:SetNowCooking(false)
								local wnd = ItemCookingWnd(UISelf)
								wnd:SendReqItemCooking(true)
								wnd:ClearCookingData()
							else
								local iH = fDelta/2*kSize:GetY()
								local iMinH = math.min(iH, kSize:GetY())
								kSize:SetY(iMinH)
								UISelf:SetImgSize(kSize)
							end
						</SCRIPT>
