	<FORM ID="FRM_SUMMON_LIST" ALIGN_X="0" Y="200" W="143" H="24" CAN_DRAG="1" >
		<ATTR IMG="../Data/6_UI/skill/skNewUITab.tga" IMG_W="143" IMG_H="24"/>
		<ATTR TEXT="792100" T_X="4" T_Y="3" T_LEFT="1" FONT="Font_Chat" T_BOLD="1" FONT_COLOR_HEX="0xffFFF568" T_OUTLINE="1" OUTLINE_COLOR_HEX="0xFF000000"/>
		<BUTTON ID="BTN_MINIMIZE" X="122" Y="3" W="20" H="21">
			<ATTR BTN_IMG="../Data/6_ui/skill/skArwDw.tga" BTN_IMG_W="40" BTN_IMG_H="42"/>
			<SCRIPT ID="ON_L_BTN_DOWN"> SummonList_Minimize(UISelf) </SCRIPT>
		</BUTTON>
		<LIST ID="LST_SUMMON_INFO" Y="23">
			<ATTR ELEMENT_ID="FRM_SUMMON_INFO" ITEM_X="0" ITEM_Y="1"/>
		</LIST>
		<SCRIPT ID="ON_TICK">SummonList_Tick()</SCRIPT>
		<SCRIPT ID="ON_CLOSE">	CloseToolTip()	</SCRIPT>
		<!-- Save UIPos and Load UIPos-->
		<SCRIPT ID="ON_CALL"> 
			local PosX = Config_GetValue(HEADKEY_USER_UI, SUBKEY_SUMMON_UI_POS_X)
			local PosY = Config_GetValue(HEADKEY_USER_UI, SUBKEY_SUMMON_UI_POS_Y)
			if(0 ~= PosX and 0 ~= PosY) then
				UISelf:SetLocation( Point2(PosX, PosY) )
			end
		</SCRIPT>
		<SCRIPT ID="ON_L_BTN_UP">
			local kPos = UISelf:GetLocation()
			Config_SetValue(HEADKEY_USER_UI, SUBKEY_SUMMON_UI_POS_X, kPos:GetX())
			Config_SetValue(HEADKEY_USER_UI, SUBKEY_SUMMON_UI_POS_Y, kPos:GetY())
			Config_ApplyConfig()
			Config_Save(false)
		</SCRIPT>
	</FORM>
