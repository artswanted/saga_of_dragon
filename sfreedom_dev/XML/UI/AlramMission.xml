<?xml version="1.0" encoding="euc-kr"?>
<XUI>
	<FORM ID="FRM_ALRAM_MISSION" ALIGN_X="100" Y="55" W="247" H="120" IS_ABSOLUTE_X="1" IS_ABSOLUTE_Y="1" CAN_DRAG="0">
		<SCRIPT ID="ON_DISPLAY">
			if( CheckNil(g_world == nil) ) then return false end
			if( CheckNil(g_world:IsNil()) ) then return false end
			local UIPos = UISelf:GetLocation()
			if GATTR_BATTLESQUARE == g_world:GetAttr() then
				UISelf:SetLocation(Point2(0, 156))
			else
				local iScreenX = GetScreenSize():GetX()
				local iPosY = 80
				if iScreenX >= 1600 then
					iPosY = 0
				elseif iScreenX == 1024 then
					iPosY = 55
				end
				UISelf:SetAlignX(100)
				local kPos = UISelf:GetLocation()
				UISelf:SetLocation(Point2(kPos:GetX(), iPosY))
			end
		</SCRIPT>
