			<ATTR WND_BAR="ScrollBg" BTN_UP="ScrUp" BTN_DOWN="ScrDown" BTN_MIDDLE="ScrMiddle"/>
			<SCRIPT ID="ON_CALL">
				local kSelf = UISelf
				kSelf:ClearAllListItem()				
				local Item = kSelf:AddNewListItemChar("")
				if false == Item:IsNil() then
					local ItemWnd = Item:GetWnd()
					local TextWnd = ItemWnd:GetControl("FRM_TEXT")
					if false == TextWnd:IsNil() then						
						TextWnd:SetStaticText(GetTT(799004):GetStr())
						SetUISizeForTextY(TextWnd)
						local kSize = TextWnd:GetSize()
						kSize:SetY(kSize:GetY()+50)
						ItemWnd:SetSize(kSize)
						ItemWnd:SetInvalidate()
					end
				end
			</SCRIPT>
		</LIST>
