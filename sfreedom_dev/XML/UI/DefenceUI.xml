						UVAnimation(UISelf, 0.09, 1, 7, 1)
					end
				</SCRIPT>
			</FORM>
			<ICON ID="ICN_ITEM" X="4" Y="4" Z="10" W="40" H="40" ICON_GRP="254">
				<SCRIPT ID="ON_CALL">
					UISelf:SetCustomDataAsInt(299603120)
					local itemNo = GetDefence7PointCopyItem()
					if 0 ~= itemNo then
						UISelf:SetGrayScale(0)
					else
						UISelf:SetGrayScale(1)
					end
				</SCRIPT>
				<SCRIPT ID="ON_DISPLAY">
					UISelf:DrawIcon()
				</SCRIPT>
				<SCRIPT ID="ON_L_BTN_UP">
					local itemNo = GetDefence7PointCopyItem()
					if 0 ~= itemNo then
						DoQuickUseItem(itemNo)
					else
						AddWarnDataTT(750037)
						UISelf:SetGrayScale(1)
					end
					itemNo = GetDefence7PointCopyItem()
					if 0 ~= itemNo then
						UISelf:SetGrayScale(0)
					end
					return true
				</SCRIPT>
				<SCRIPT ID = "ON_MOUSE_OVER"> UISelf:CallIconToolTip() </SCRIPT>
				<SCRIPT ID = "ON_MOUSE_OUT"> CloseToolTip() </SCRIPT>
			</ICON>
