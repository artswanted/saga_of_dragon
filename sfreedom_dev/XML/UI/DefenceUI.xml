	<FORM ID="FRM_POINT_COPY_MACHINE" ALIGN_X="100" ALIGN_Y="40" W="222" H="174" CAN_DRAG="1">
		<ATTR IMG="../Data/6_ui/mission/msF7Box.tga" IMG_W="222" IMG_H="174"/>
		<SCRIPT ID="ON_CALL">
			local Info = GetUIWnd("FRM_DEFENCE_STAGE_INFO")
			if Info:IsNil() then return end

			local Point = Info:GetLocation()
			local Size = Info:GetSize()
			UISelf:SetLocation(Point2(Point:GetX(), Point:GetY() + Size:GetY()))
			
			g_CopyMaxVisible = false
		</SCRIPT>
		<SCRIPT ID="ON_TICK">
			if g_UsingCopyMachine then
				UISelf:GetControl("SFRM_ARROW_TARGET"):Visible(false)
			else
