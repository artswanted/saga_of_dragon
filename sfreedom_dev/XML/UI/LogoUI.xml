			elseif LOCALE.NC_PHILIPPINES == GetLocale() then
				kAliveTime = 4000
			elseif LOCALE.NC_KOREA == GetLocale() then
				kAliveTime = 4000
				local w = Config_GetValue("GAME", "RESOLUTION_WIDTH")
