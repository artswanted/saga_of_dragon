<XUI>
	<FORM ID="FRM_LOGO_IMG" X="0" Y="0" W="1600" H="1200" PRIORITY="0" IS_ABSOLUTE_X="1" IS_ABSOLUTE_Y="1">
		<ATTR IMG="../Data/6_ui/Common/cmBody.tga" IMG_W="1600" IMG_H="1200"/>
		<SCRIPT ID = "ON_CALL">
			local slf = UISelf
			slf:SetSize(Point2(GetScreenSize():GetX(),GetScreenSize():GetY()))
			slf:SetImgSize(Point2(GetScreenSize():GetX(),GetScreenSize():GetY()))
			slf:SetLocation(Point2(0,0))
			if false == g_bIsFirstJoin then return end
			
			if LOCALE.NC_DEVELOP == GetLocale() then
				CallUI("FRM_BARUNSON_LOGO")
				CallUI("FRM_BARUNSON_COPYRIGHT")
			elseif LOCALE.NC_USA == GetLocale() 
				or LOCALE.NC_THAILAND == GetLocale()
			then
				CallUI("FRM_NOTICE_MAIN")
				CallUI("FRM_NOTICE_COPY")
				CallUI("FRM_NOTICE_LOGO")
			elseif LOCALE.NC_KOREA == GetLocale()
			then
				CallUI("FRM_NOTICE_MAIN")
				CallUI("FRM_NOTICE_MARK")
			else
				CallUI("FRM_PUBLISHER_MAIN")
				CallUI("FRM_PUBLISHER_COPY")
				CallUI("FRM_PUBLISHER_LOGO")
			end
		</SCRIPT>
		<SCRIPT ID="ON_CLOSE">
			CloseUI("FRM_BARUNSON_LOGO")
			CloseUI("FRM_BARUNSON_COPYRIGHT")
		</SCRIPT>
	</FORM>
	
