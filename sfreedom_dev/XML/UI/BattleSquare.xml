				<SCRIPT ID="ON_CALL">
					g_fAutoExitLimitTime = GetAccumTime()
					g_fAutoExitTime = 60
				</SCRIPT>
				<SCRIPT ID="ON_TICK">
					if g_fAutoExitLimitTime ~= nil then
						if GetAccumTime() - g_fAutoExitLimitTime >= 1 then
							g_fAutoExitLimitTime = GetAccumTime()
							g_fAutoExitTime = g_fAutoExitTime - 1
							if g_fAutoExitTime > 0 then
								local sdw = UISelf:GetControl("FRM_MESSAGE")
								if false == sdw:IsNil() then
									sdw:SetStaticText( string.format(GetTextW(401057):GetStr(), g_fAutoExitTime) )
								end
							else
								Net_RecentMapMove()
								return
							end
						end
					end
				</SCRIPT>
