  <STATIC_FORM ID="SFRM_GAMEPAD" ALIGN_X="50" Y="90" W="826" H="480" CAN_DRAG="1" STATIC_NUM="1" >
	<SCRIPT ID="ON_CALL">
	  if false == GetGamePadUI():IsPadExist() then
		 CallUI("SFRM_PADNOEXIST", 1)
		 return
	  end
	  GetGamePadUI():BackupSetting()
	  for i=1, 24 do
		local PadButtonName = GetGamePadUI():GetPadButtonBindWithUIButton(i);
		local UIFrameName = "SFRM_BUTTONFRAME"..i
		local UIButtonName = "BTN_INDEX"..i
	  
		UISelf:GetControl("SFRM_BACK"):GetControl(UIFrameName):GetControl(UIButtonName):SetStaticText(PadButtonName:GetStr())
	  end
	</SCRIPT>
	<SCRIPT ID="ON_TICK">
	  local iTwinkelButtonIndex = GetGamePadUI():GetUIButtonIndexByPressedPadButton()
	  if 0 ~= iTwinkelButtonIndex then
		local UIFrameName = "SFRM_BUTTONFRAME"..iTwinkelButtonIndex
		local UIButtonName = "BTN_INDEX"..iTwinkelButtonIndex
		UISelf:GetControl("SFRM_BACK"):GetControl(UIFrameName):GetControl(UIButtonName):SetTwinkle(1000, 100)
		GetGamePadUI():UnbindButton(iButtonIndex);
	  end

	</SCRIPT>
