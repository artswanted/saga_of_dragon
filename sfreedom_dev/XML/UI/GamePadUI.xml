	</SCRIPT>
	<SCRIPT ID="ON_TICK">
	  local kButtonName = GetGamePadUI():GetPadButtonNameOnTextTableAndBindPadButton()
	  if "" ~= kButtonName:GetStr() then
		local iButtonIndex = GetGamePadUI():GetPressedButtonIndex()
		local UIFrameName = "SFRM_BUTTONFRAME"..iButtonIndex
		local UIButtonName = "BTN_INDEX"..iButtonIndex

		local iUnbindButtonIndex = GetGamePadUI():GetUnbindedUIButtonIndex();
		local UnbindFrameName = "SFRM_BUTTONFRAME"..iUnbindButtonIndex;
		local UnbindButtonName = "BTN_INDEX"..iUnbindButtonIndex;
		
		GetUIWnd("SFRM_GAMEPAD"):GetControl("SFRM_BACK"):GetControl(UnbindFrameName):GetControl(UnbindButtonName):SetStaticText(GetTT(90001000):GetStr())
		GetGamePadUI():EraseUnbindButtonInfo()
		GetUIWnd("SFRM_GAMEPAD"):GetControl("SFRM_BACK"):GetControl(UIFrameName):GetControl(UIButtonName):SetStaticText(kButtonName:GetStr())
		
		CloseUI("SFRM_BUTTONLISTNER")
	  end
	</SCRIPT>

