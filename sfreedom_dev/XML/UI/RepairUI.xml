		<BUTTON ID = "BTN_REPAIR">
			<SCRIPT ID="ON_CREATE">	SetTextOnThisUI(400605, 2200)	</SCRIPT>
			<ATTR X="51" Y="197" W="84" H="26"/>
			<ATTR BTN_IMG="../Data/6_ui/common/cmBtBg.tga" BTN_IMG_W="168" BTN_IMG_H="52"/>
			<ATTR TEXT = "" FONT = "FTbt_12" T_X="42" T_Y ="3" T_CENTER = "1" BTN_ENABLE_CHANGE_COLOR="1" BTN_T_COLOR_TYPE="10"/>
			<SCRIPT ID = "ON_L_BTN_DOWN"> 
				local btn = ItemRepairWnd(UISelf)
				local err = btn:CheckOK()
				if 0 ~= err then
					AddWarnDataTT(err)
					return
				end
				local wnd = UISelf:GetParent():GetControl("SFRM_COLOR1"):GetControl("SFRM_SHADOW"):GetControl("SFRM_SINGLE"):GetControl("CBTN_CHECK") 
				local wnd1 = UISelf:GetParent():GetControl("SFRM_COLOR1"):GetControl("SFRM_ALL"):GetControl("CBTN_CHECK") 
				if wnd:IsNil() == false and wnd:GetCheckState() == true then
					CallUI("SFRM_MSG_REPAIR_SINGLE")
				elseif wnd1:IsNil() == false and wnd1:GetCheckState() == true then
					CallUI("SFRM_MSG_REPAIR_ALL")
				end
			</SCRIPT>
			<HOTKEY ID = "59">
				local btn = ItemRepairWnd(UISelf)
				local err = btn:CheckOK()
				if 0 ~= err then
					AddWarnDataTT(err)
					return true
				end
				local wnd = UISelf:GetParent():GetControl("SFRM_COLOR1"):GetControl("SFRM_SHADOW"):GetControl("SFRM_SINGLE"):GetControl("CBTN_CHECK") 
				local wnd1 = UISelf:GetParent():GetControl("SFRM_COLOR1"):GetControl("SFRM_ALL"):GetControl("CBTN_CHECK") 
				if wnd:IsNil() == false and wnd:GetCheckState() == true then
					CallUI("SFRM_MSG_REPAIR_SINGLE")
				elseif wnd1:IsNil() == false and wnd1:GetCheckState() == true then
					CallUI("SFRM_MSG_REPAIR_ALL")
				end
				return true
			</HOTKEY>
		</BUTTON>
