		<ATTR ALIGN_X = "100" ALIGN_Y = "50" W = "128" H = "135" PASS_EVENT = "1" IGNORE_TWINKLE="1"/>
		<SCRIPT ID = "ON_CALL">
			local kActor = GetPlayer()
			if true == kActor:IsNil() then
				return
			end
			local	iBaseClassID = kActor:GetPilot():GetBaseClassID()
			local Arr = {}
			Arr[1] = "FRM_FIGHTER"
			Arr[2] = "FRM_MAGICIAN"
			Arr[3] = "FRM_ARCHER"
			Arr[4] = "FRM_THIFE"
			Arr[51] = "FRM_SHAMAN"
			Arr[52] = "FRM_DOUBLE_FIGHTER"
			for i=1,4 do
				local wndClass = UISelf:GetControl(Arr[i])
				if false==wndClass:IsNil() then
					wndClass:Visible(i==iBaseClassID)					
				end
			end
			for i=51,52 do
				local wndClass = UISelf:GetControl(Arr[i])
				if false==wndClass:IsNil() then
					wndClass:Visible(i==iBaseClassID)					
				end
			end
		</SCRIPT>
