	<FORM ID="FRM_HELP_MINI">
		<ATTR X="506" Y="564" W="508" H="127"/>
		<ATTR IMG="../Data/6_ui/main/mnHelpBg.tga" IMG_W="508" IMG_H="127"/>
		<SCRIPT ID="ON_CALL">
			if( CheckNil(g_world == nil) ) then return false end
			if( CheckNil(g_world:IsNil()) ) then return false end
			FHM_PrevTime = g_world:GetAccumTime()
		</SCRIPT>
		<FORM ID="FRM_BIG_ICON" X="12" Y="13" W="60" H="63">
			<ATTR IMG="../Data/6_ui/main/mnHelpIcon.tga" IMG_W="120" IMG_H="126" U="2" V="2" UV_INDEX="3"/>
			<SCRIPT ID="ON_L_BTN_DOWN"> CallUI(UISelf:GetCustomDataAsStr():GetStr()) </SCRIPT>
			<SCRIPT ID="ON_DISPLAY">
				local FHM_UV = UISelf:GetUVIndex()
				if FHM_UV ~= 3 then
					if( CheckNil(g_world == nil) ) then return false end
					if( CheckNil(g_world:IsNil()) ) then return false end
					local FHM_NextTime = g_world:GetAccumTime()
					if FHM_NextTime - FHM_PrevTime > 1 then
						FHM_PrevTime = FHM_NextTime
						if FHM_UV == 1 then
							UISelf:SetUVIndex(FHM_UV+1)
						else
							UISelf:SetUVIndex(FHM_UV-1)
						end
					end
				end
			</SCRIPT>
		</FORM>
